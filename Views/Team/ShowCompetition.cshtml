@{
    ViewData["Title"] = "Hackathon";
}

@model Hackathon.Models.Competition

<h2>@Model.CompetitionName</h2>
<h4><span class="show-time">START:</span>@Model.Start.ToString("f")</h4>
<h4><span class="show-time">END:</span>@Model.End.ToString("f")</h4>
<hr>
<p class="orangemessage">@ViewBag.message</p>
<h4>Teams</h4>
<ul>
@foreach (var t in Model.Teams){
    <li>@t.TeamName - @t.ProjectTitle
        <ul>
        @foreach (var s in t.Students) {
            <li>  @s.UserInfo.FirstName  </li>
        }
        </ul>
    </li>
}
</ul>