@{
    ViewData["Title"] = "Admin";
}
<h3>Add Students</h3>

<form asp-controller="Admin" asp-action="UploadStudents" method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <label for="">Upload a csv file:</label>
        <input type="file" name="students" class="form-control">
        <input type="submit" value="Upload Students">
    </div>
</form>